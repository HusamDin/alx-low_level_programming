#include <stdio.h>

/**
 * times_table - Prints the 9 times table
 *
 *Return: 0
 */

void times_table(void)
{
	int fn, ln, prod = 0;

	for (fn = 0; fn <= 9; fn++)
	{
		for (ln = 0; ln <= 9; ln++)
		{
			prod = fn * ln;
			if (ln == 9)
			{
				printf("%d", prod);
			}
			else
				printf("%d, ", prod);
		}
		putchar('\n');
	}
}
